{
    "model": "efficient_net",
    "input_info": {
      "sample_size": [1, 3, 32, 32]
    },
    "train_steps": 100,
    "multiprocessing_distributed": false,
    "optimizer": {
        "type": "sgd",
        "no_decay_keys": "bn#bias",
        "base_lr": 2.5e-6,
        "weight_decay": 3e-7,
        "label_smoothing": 0.1
    },
    "bootstrapNAS": {
        "training": {
            "algorithm": "progressive_shrinking",
            "batchnorm_adaptation": {
                "num_bn_adaptation_samples": 2
            },
            "schedule": {
                "list_stage_descriptions": [
                    {"train_dims": ["depth"], "epochs": 1},
                    {"train_dims": ["depth", "width"], "epochs": 1, "width_indicator": 4},
                    {"train_dims": ["depth", "width"], "epochs": 1, "reorg_weights": true, "width_indicator": 1}
                ]
            },
            "elasticity": {
                "available_elasticity_dims": ["depth","width"],
                "depth": {
                    "mode": "manual",
                    "skipped_blocks": [
                        ["EfficientNet/ModuleList[_blocks]/MBConvBlock[14]/MemoryEfficientSwish[_swish]/__mul___1",
                            "EfficientNet/ModuleList[_blocks]/MBConvBlock[14]/__mul___0"],
                        ["EfficientNet/ModuleList[_blocks]/MBConvBlock[15]/MemoryEfficientSwish[_swish]/__mul___1",
                            "EfficientNet/ModuleList[_blocks]/MBConvBlock[15]/__mul___0"]
                    ]
                }
            }
        },
        "search": {
            "algorithm": "NSGA",
            "num_evals": 2,
            "population": 1
        }
    },
    "model_params": {
        "model_name": "efficientnet-b0"
    }
}
