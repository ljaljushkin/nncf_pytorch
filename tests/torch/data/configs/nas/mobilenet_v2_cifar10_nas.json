{
    "model": "mobilenet_v2_cifar10",
    "input_info": {
        "sample_size": [1, 3, 32, 32]
    },
    "multiprocessing_distributed": false,
    "bootstrapNAS": {
        "training": {
            "algorithm": "progressive_shrinking",
            "progressivity_of_elasticity": ["kernel", "depth", "width"],
            "batchnorm_adaptation": {
                "num_bn_adaptation_samples": 2
            },
            "schedule": {
                "list_stage_descriptions": [
                    {"train_dims": ["depth"], "epochs": 1},
                    {"train_dims": ["depth"], "epochs": 1, "depth_indicator": 2},
                    {"train_dims": ["depth", "width"], "epochs": 1, "depth_indicator": 2, "reorg_weights": true, "width_indicator": 2}
                ]
            },
            "elasticity": {
                "enabled_elasticity_dims": ["width", "depth"],
                "width": {
                    "min_out_channels": 50,
                    "width_step": 32,
                    "filter_importance": "L1"
                },
                "kernel": {
                    "max_num_kernels": "3"
                }
            }
        },
        "search": {
            "algorithm": "NSGA"
        },
        "compression": [
            {
                "algorithm": "quantization",
                "params": {},
                "initializer": {}
            }
        ]
    }
}
