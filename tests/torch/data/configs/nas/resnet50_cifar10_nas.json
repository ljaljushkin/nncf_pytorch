{
    "model": "resnet50_cifar10",
    "input_info": {
      "sample_size": [1, 3, 32, 32]
    },
    "multiprocessing_distributed": false,
    "bootstrapNAS": {
        "training": {
            "algorithm": "progressive_shrinking",
            "batchnorm_adaptation": {
                "num_bn_adaptation_samples": 2
            },
            "schedule": {
                "list_stage_descriptions": [
                    {"train_dims": ["width"], "epochs": 1, "width_indicator": 4},
                    {"train_dims": ["width"], "epochs": 1, "reorg_weights": true, "width_indicator": 1}
                ]
            },
            "elasticity": {
                "enabled_elasticity_dims": ["width"]
            }
        }
    }
}
