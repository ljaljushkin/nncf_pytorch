{
    "model": "efficient_net",
    "num_classes": 100,
    "dataset": "cifar100_224x224",
    "input_info": {
        "sample_size": [1, 3, 224, 224]
    },
    "batch_size": 64,
    "batch_size_val": 2000,
    "multiprocessing_distributed": false,

    "optimizer": {
        "type": "sgd",
        "momentum": 0.9,
        "nesterov": true,
        "weight_decay": 3e-7,
        "base_lr": 1.0, // 2.5e-6,
        "label_smoothing": 0.1,
        "no_decay_keys": "bn#bias",
//        "lr_schedule_type": "cosine"
    },
    "bootstrapNAS": {
        "training": {
            "algorithm": "progressive_shrinking",
            "progressivity_of_elasticity": ["kernel", "depth", "width"],
            "batchnorm_adaptation": {
                "num_bn_adaptation_samples": 1500
            },
            "schedule": {
                "list_stage_descriptions": [
                    {"train_dims": ["kernel"], "epochs": 10, "init_lr": 2.5e-6, "epochs_lr": 10},
                    {"train_dims": ["kernel","depth"], "epochs": 10, "depth_indicator": 1, "init_lr": 2.5e-6, "epochs_lr": 10},
                    {"train_dims": ["kernel","depth"], "epochs": 10, "depth_indicator": 2, "init_lr": 2.5e-6, "epochs_lr": 10},
                    {"train_dims": ["kernel","depth", "width"], "epochs": 10, "reorg_weights": true, "width_indicator": 2, "bn_adapt": true, "init_lr": 2.5e-6, "epochs_lr": 10},
                    {"train_dims": ["kernel","depth", "width"], "epochs": 10, "reorg_weights": true, "width_indicator": 3, "bn_adapt": true, "init_lr": 2.5e-6, "epochs_lr": 10}
                ]
            },
            "elasticity": {
                "available_elasticity_dims": ["width", "depth", "kernel"],
                "width": {
                    "max_num_widths": 3,
                      "min_out_channels": 32,
                      "width_step": 32,
                      "width_multipliers": [1, 0.85, 0.60]
                },
                "kernel": {
                    "max_num_kernels": 3
                }
            },
        },
        "search": {
            "algorithm": "NSGA",
            "num_evals": 1000,
            "population": 40,
        }
    },
    "model_params": {
        "model_name": "efficientnet-b0"
    }
}
