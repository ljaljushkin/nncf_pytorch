        "sample_size": [
            1,
            3,
            224,
            224
        ]
    },
    "batch_size": 256,
    "batch_size_val": 256,
    "multiprocessing_distributed": false,
    "optimizer": {
        "type": "sgd",
        "momentum": 0.9,
