{
    "model": "efficient_net",
    "num_classes": 100,
    "dataset": "cifar100_224x224",
    "input_info": {
        "sample_size": [1, 3, 224, 224]
    },
    "print_freq": 1000,
    "batch_size": 64,
    "batch_size_val": 256,
    "multiprocessing_distributed": false,
    "optimizer": {
         "step": 81
     },
    "bootstrapNAS": {
        "training": {
            "algorithm": "progressive_shrinking",
            "progressivity_of_elasticity": ["kernel", "depth", "width"],
            "batchnorm_adaptation": {
                "num_bn_adaptation_samples": 1
            },
            "schedule": {
                "list_stage_descriptions": [
                    {"train_dims": ["kernel"], "epochs": 20},
                    {"train_dims": ["kernel","depth"], "epochs": 20, "depth_indicator": 1},
                    {"train_dims": ["kernel","depth"], "epochs": 20, "depth_indicator": 2},
                    {"train_dims": ["kernel","depth", "width"], "epochs": 20, "reorg_weights": true, "width_indicator": 2, "bn_adapt": true}
                ]
            },
            "elasticity": {
                "available_elasticity_dims": ["width", "depth", "kernel"],
                "width": {
                    "max_num_widths": 2,
                      "min_out_channels": 32,
                      "width_step": 32,
                      "width_multipliers": [1, 0.85]
                },
                "kernel": {
                    "max_num_kernels": 3
                }
            }
        },
    },
    "model_params": {
        "model_name": "efficientnet-b0"
    }
}
