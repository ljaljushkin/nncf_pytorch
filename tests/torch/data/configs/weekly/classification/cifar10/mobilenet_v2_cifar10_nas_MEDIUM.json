{
    "model": "mobilenet_v2_cifar10",
    "num_classes": 10,
    "dataset": "CIFAR10",
    "input_info": {
        "sample_size": [1, 3, 32, 32]
    },
    "batch_size": 64,
    "batch_size_val": 2000,
    "multiprocessing_distributed": false,
    "optimizer": {
        "type": "sgd",
        "momentum": 0.9,
        "nesterov": true,
        "weight_decay": 3e-7,
        "base_lr": 1.0, // 2.5e-6,
        "label_smoothing": 0.1,
        "no_decay_keys": "bn#bias",
//        "lr_schedule_type": "cosine"
    },
    "bootstrapNAS": {
        "training": {
            "algorithm": "progressive_shrinking",
            "progressivity_of_elasticity": ["kernel", "depth", "width"],
            "batchnorm_adaptation": {
                "num_bn_adaptation_samples": 1500
            },
//            "lr_schedule": {
//                "params": {
//                    "base_lr": 2.5e-6
//                },
//            },
            "schedule": {
                "list_stage_descriptions": [
                    {"train_dims": ["depth"], "epochs": 10, "depth_indicator": 1, "init_lr": 2.5e-6, "epochs_lr": 10},
                    {"train_dims": ["depth", "width"], "epochs": 10, "reorg_weights": true, "width_indicator": 2, "bn_adapt": true, "init_lr": 2.5e-6, "epochs_lr": 10},
                    {"train_dims": ["depth", "width"], "epochs": 10, "reorg_weights": true, "width_indicator": 3, "bn_adapt": true, "init_lr": 2.5e-6, "epochs_lr": 10}
                ]
            },
            "elasticity": {
                "available_elasticity_dims": ["width", "depth"],
                "width": {
                    "max_num_widths": 2,
                      "min_out_channels": 32,
                      "width_step": 32,
                      "width_multipliers": [1, 0.85, 0.60]
                },
                "kernel": {
                    "max_num_kernels": 3
                },
                "depth": {
                    "mode": "manual",
                    "skipped_blocks": [
                //        [
                //             0
                //            "MobileNetV2/Sequential[features]/InvertedResidual[2]/Sequential[conv]/NNCFConv2d[2]/conv2d_0",
                //            "MobileNetV2/Sequential[features]/InvertedResidual[3]/__add___0"
                //        ],
                //        [
                //             1
                //            "MobileNetV2/Sequential[features]/InvertedResidual[4]/Sequential[conv]/NNCFConv2d[2]/conv2d_0",
                //            "MobileNetV2/Sequential[features]/InvertedResidual[5]/__add___0"
                //        ],
                //        [
                //            // 2
                //            "MobileNetV2/Sequential[features]/InvertedResidual[7]/Sequential[conv]/NNCFConv2d[2]/conv2d_0",
                //            "MobileNetV2/Sequential[features]/InvertedResidual[8]/__add___0"
                //        ],
                        [
                //             3 -> 0
                            "MobileNetV2/Sequential[features]/InvertedResidual[11]/Sequential[conv]/NNCFConv2d[2]/conv2d_0",
                            "MobileNetV2/Sequential[features]/InvertedResidual[12]/__add___0"
                        ],
                        [
                //             4 -> 1
                            "MobileNetV2/Sequential[features]/InvertedResidual[14]/Sequential[conv]/NNCFConv2d[2]/conv2d_0",
                            "MobileNetV2/Sequential[features]/InvertedResidual[15]/__add___0"
                        ]
                    ],
                }
            },
        },
        "search": {
            "algorithm": "NSGA",
            "num_evals": 1000,
            "population": 40,
            "use_cache_file": true,
//            "cache_file_path":
        }
    }
}
