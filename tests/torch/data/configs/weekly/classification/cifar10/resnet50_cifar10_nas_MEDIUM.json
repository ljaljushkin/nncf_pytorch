{
    "model": "resnet50_cifar10",
    "num_classes": 10,
    "dataset": "CIFAR10",
    "input_info": {
        "sample_size": [1, 3, 32, 32]
    },
    "batch_size": 64,
    "batch_size_val": 2000,
    "multiprocessing_distributed": false,
    "bootstrapNAS": {
        "training": {
            "algorithm": "progressive_shrinking",
            "progressivity_of_elasticity": ["kernel", "depth", "width"],
            "batchnorm_adaptation": {
                "num_bn_adaptation_samples": 1500
            },
            "schedule": {
                "list_stage_descriptions": [
                    {"train_dims": ["depth"], "epochs": 10, "depth_indicator": 1},
                    {"train_dims": ["depth"], "epochs": 10, "depth_indicator": 2},
                    {"train_dims": ["depth", "width"], "epochs": 10, "reorg_weights": true, "width_indicator": 2, "bn_adapt": true}
                ]
            },
            "elasticity": {
                "enabled_elasticity_dims": ["width", "depth"],
                "width": {
                    "max_num_widths": 2,
                      "min_out_channels": 32,
                      "width_step": 32,
                      "use_width_multiplier": true,
                      "width_multipliers": [1, 0.85]
                },
                "kernel": {
                    "max_num_kernels": "3"
                }
            }
        },
        "search": {
            "algorithm": "NSGA",
            "evaluations": 1000
        },
        "compression": [
            {
                "algorithm": "quantization",
                "params": {},
                "initializer": {}
            }
        ]
    }
}
