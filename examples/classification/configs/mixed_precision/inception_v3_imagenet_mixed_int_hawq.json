{
    "model": "inception_v3",
    "pretrained": true,
    "input_info": {
      "sample_size": [2, 3, 299, 299]
    },
    "num_classes": 1000,
    "batch_size" : 100,
    "multiprocessing_distributed" : true,
    "optimizer": {
        "base_lr": 3.1e-5,
        "schedule_type": "plateau",
        "type": "Adam",
        "scheduler_params": {
            "threshold": 0.1,
            "cooldown": 3
        },
        "weight_decay": 1e-05
    },
    "target_device": "VPU",
    "compression": {
        "algorithm": "quantization",
        "initializer": {
            "range": {
                "num_init_samples": 160
            },
            "precision": {
                "type": "hawq",
                "bits": [4,8],
                "compression_ratio": 1.5
            }
        },
        "ignored_scopes": [
            "Inception3/__add___0",
            "Inception3/__add___1",
            "Inception3/__add___2",
            "Inception3/__mul___0",
            "Inception3/__mul___1",
            "Inception3/__mul___2",
            "Inception3/cat_0"
        ]
    }
}
