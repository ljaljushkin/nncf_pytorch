{
    "model": "efficient_net",
    "num_classes": 100,
    "dataset": "cifar100_224x224",
    "input_info": {
        "sample_size": [1, 3, 224, 224]
    },
    "print_freq": 1000,
    "batch_size": 256,
    "batch_size_val": 256,
    "multiprocessing_distributed": false,
    "optimizer": {
        "type": "SGD",
        "base_lr": 0.1,
        "weight_decay": 1e-4,
        "schedule_type": "multistep",
        "steps": [
            120,
            135,
            150,
            160
        ],
        "optimizer_params": {
            "momentum": 0.9,
            "nesterov": true
        }
    },
    "bootstrapNAS": {
        "training": {
            "algorithm": "progressive_shrinking",
            "progressivity_of_elasticity": ["width"],
            "batchnorm_adaptation": {
                "num_bn_adaptation_samples": 1500
            },
            "schedule": {
                "list_stage_descriptions": [
                    {"train_dims": ["width"], "epochs": 20, "reorg_weights": true, "width_indicator": 2, "bn_adapt": true},
                    {"train_dims": ["width"], "epochs": 20, "reorg_weights": true, "width_indicator": 3, "bn_adapt": true},
                    {"train_dims": ["width"], "epochs": 20, "reorg_weights": true, "width_indicator": 4, "bn_adapt": true},
                    {"train_dims": ["width"], "epochs": 20, "reorg_weights": true, "width_indicator": 6, "bn_adapt": true},
                    {"train_dims": ["width"], "epochs": 40, "reorg_weights": true, "width_indicator": -1, "bn_adapt": true},
                ]
            },
            "elasticity": {
                "available_elasticity_dims": ["width"],
                "width": {
                    "width_multipliers": [1, 0.95, 0.9, 0.85, 0.8, 0.75, 0.7, 0.65, 0.6, 0.55, 0.5]
                }
            }
        }
    },
    "model_params": {
        "model_name": "efficientnet-b0"
    }
}
