{
    "model": "efficient_net",
    "num_classes": 100,
    "dataset": "cifar100_224x224",
    "input_info": {
        "sample_size": [
            1,
            3,
            224,
            224
        ]
    },
    "print_freq": 1000,
    "batch_size": 256,
    "batch_size_val": 256,
    "multiprocessing_distributed": false,
    "optimizer": {
        "type": "SGD",
        "base_lr": 0.1,
        "weight_decay": 1e-4,
        "schedule_type": "multistep",
        "steps": [
            20,
            40,
            60,
            80
        ],
        "optimizer_params": {
            "momentum": 0.9,
            "nesterov": true
        }
    },
    "compression": [
        {
            "algorithm": "filter_pruning",
            "pruning_init": 0.01,
            "params": {
                "schedule": "exponential",
                "pruning_target": 0.1,
                "pruning_steps": 15,
                "filter_importance": "L2"
            }
        }
    ],
    "model_params": {
        "model_name": "efficientnet-b0"
    }
}
