{
  "model": "ssd_vgg",
  "batchnorm": true,
  "input_info": {
    "sample_size": [8, 3, 512, 512]
  },
  "pretrained": true,
  "num_classes": 21,
  "dataset": "voc",
  "preprocessing": {
    "mean": [0.406, 0.456, 0.485],
    "std": [0.255, 0.224, 0.229],
    "normalize_coef": 255,
    "rgb": true
  },
  "max_iter": 5000,
  "batch_size": 24,
  "iter_size": 1,
  "save_freq": 3, // in epochs
  "optimizer": {
        "base_lr": 0.00031,
        "schedule_type": "plateau",
        "type": "Adam",
        "scheduler_params": {
            "threshold": 0.1,
            "cooldown": 3
        },
        "weight_decay": 5e-4
    },
  "ssd_params": {
    "steps": [8, 16, 32, 64, 128, 256, 512],
    "min_sizes": [35.84, 76.8, 153.6, 230.4, 307.2, 384.0, 460.8],
    "max_sizes": [76.8, 153.6, 230.4, 307.2, 384.0, 460.8, 537.6],
    "aspect_ratios": [[2], [2, 3], [2, 3], [2, 3], [2, 3], [2], [2]],
    "variance": [0.1, 0.1, 0.2, 0.2],
    "clip": false,
    "flip": true
  },
  "target_device": "VPU",
  "compression": {
    "algorithm": "quantization",
    "initializer": {
      "range": {
        "num_init_samples": 640
      },
      "precision": {
        "type": "hawq",
        "bits": [4,8],
        "compression_ratio": 1.5,
        "num_data_points": 100,
        "tolerance": 0.001,
        "dump_hawq_data": true
      }
    }
  }
}
